{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "https://crud-api-five-mauve.vercel.app"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "staticFiles": {
    "cacheControl": "public, max-age=31536000, immutable"
  }
}
